@model YonazoneWorkForce.ViewModel.AllComputersViewModel
@{
    ViewData["Title"] = "All Computer";
}
<h2>@ViewData["Title"].</h2>

<p>These are Lists of computers avilable.</p>
<hr />
    <div class="row">
        <div class="col-md-3" style="font: 1.3em bold italic;">Name of The Computer</div>
        <div class="col-md-3" style="font: 1.3em bold italic;">PurchasePrice of The Computer</div>
    </div><hr />
@foreach (var comp in Model.Computers)
{
    <div class="row">
        <div class="col-md-3">@Html.DisplayFor(modelItem => comp.Model)</div>
        <div class="col-md-3">$@Html.DisplayFor(modelItem => comp.PurchasePrice)</div>
    </div>
}
        <img src="~/images/banner4.svg" alt="Microsoft Azure" class="img-responsive" />
   

